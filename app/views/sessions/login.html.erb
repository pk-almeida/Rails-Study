<div>
    <h2>Iniciar Sessão</h2>
    <%= form_tag '/login' do %>
        Username: <%= text_field_tag :username %><br>
        Senha: <%= password_field_tag :password %><br>
        <%= submit_tag "Fazer login" %>
    <% end %>
    <p>Não possui uma conta? <%= link_to "Cadastre-se agora!", users_new_path %></p>
</div>